# Example environment variables for running tests locally
# Copy this file to .env and update with your actual database credentials

# MySQL test database configuration
TEST_MYSQL_DB_HOST=localhost
TEST_MYSQL_DB_PORT=3306
TEST_MYSQL_DB_USERNAME=root
TEST_MYSQL_DB_PASSWORD=your_password
TEST_MYSQL_DB_DATABASE=test_db

# PostgreSQL test database configuration
TEST_POSTGRES_DB_HOST=localhost
TEST_POSTGRES_DB_PORT=5432
TEST_POSTGRES_DB_USERNAME=postgres
TEST_POSTGRES_DB_PASSWORD=your_password
TEST_POSTGRES_DB_DATABASE=test_db
